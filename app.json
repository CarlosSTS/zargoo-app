{
  "expo": {
    "name": "Zargoo",
    "slug": "zargoo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6D00"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "YOUR_GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF6D00"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.carlosts.zargoo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O Zargoo precisa de acesso às suas fotos para que você possa enviar documentos.",
          "cameraPermission": "O Zargoo precisa de acesso à sua câmera para que você possa tirar fotos de documentos."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "O Zargoo precisa de acesso à sua localização para fornecer serviços baseados em localização."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "a2957f80-59e2-4a83-b4a6-9cdd980d364a"
      }
    },
    "owner": "carlosts"
  }
}